<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="GroupingTest">
	<test name="Regression" enabled="false">
		<groups>
			<run>
				<include name="regression"></include>
				<include name="all"></include>
			</run>
		</groups>
		<classes>
			<class name="com.java.testNG.GroupingTestCases" />
		</classes>
	</test> <!-- Test -->
	<test name="Sanitary">
		<groups>
			<run>
				<include name="sanitary"></include>
				<include name="all"></include>
			</run>
		</groups>
		<classes>
			<class name="com.java.testNG.GroupingTestCases" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
